<h1>Create a new Listing</h1>
<% if flash[:notice] %>
   <div class="notice"><%= flash[:notice] %></div>
 <% end %>
<%= form_for @listing do |f| %>
  <%= f.label "Enter a description of your pet's needs" %>
  <%= f.text_field :description %>
  <br>
  <%=f.label "date"%>
  <%=f.date_field :start_date%>
  <%= f.label "Enter the price you are willing to pay" %>
  <%= f.number_field :price %>
  <br>
  ### import client's pets from session
  <%= f.submit %>
<% end %>
